    "version": "0.2.0",
    "configurations": [
        {
            "name": "wine_snob (debug mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "debug",
            "toolArgs": [
                "--dart-define-from-file",
                "${workspaceFolder}/android/keys/api-keys.json"
            ]
        },
        {
            "name": "wine_snob (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "toolArgs": [
                "--dart-define-from-file",
                "${workspaceFolder}/android/keys/api-keys.json"
            ]
        },
        {
            "name": "wine_snob (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "toolArgs": [
                "--dart-define-from-file",
                "${workspaceFolder}/android/keys/api-keys.json"
            ]
        }
    ]
}